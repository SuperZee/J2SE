package com.lee.spi_05.class_object;

public class ObjectDemo {

	public static void main(String[] args) {
		Person p = new Person();
		p.age = 18;
		p.name = "ะกร๗";
		p.talk();
		System.out.println(p);
	}

}

class Person {
	int age;
	String name;

	public void talk() {
		System.out.println("Talk");
	}

	@Override
	public String toString() {
		return "[Person] : age = "+ this.age +" name = " + this.name+"]";
	}
}