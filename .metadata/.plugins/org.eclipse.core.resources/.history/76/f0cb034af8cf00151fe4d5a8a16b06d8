package com.lee.spi_13.swing;

/**
 * JPanel 实现一个JFrame多个布局的组合
 * @author SpiColorPendra
 *
 */
import java.awt.*;
import javax.swing.*;

public class Demo5_JPanel {
	// 定义组件
	JPanel jp1, jp2;
	JButton jb1, jb2, jb3, jb4, jb5, jb6;

	public static void main(String[] args) {

	}

	public Demo5_JPanel() {
		//JPanel默认布局是流布局模式
		jp1 = new JPanel();
		jp2 = new JPanel();
		
		jb1 = new JButton("胡萝卜");
		jb2 = new JButton("菠菜");
		jb3 = new JButton("萝卜");
		jb4 = new JButton("豆干");
		jb5 = new JButton("南瓜");
		jb6 = new JButton("土豆");
	}
}
