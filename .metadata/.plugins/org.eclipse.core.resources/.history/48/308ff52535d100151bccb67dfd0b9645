package com.lee.spi_15.tankgame;

/**
 * @version 1.0
 * @author SpiColorPendra
 *
 */
public class MyTankGame {

}

// 坦克类
class Tank {
	// 表示tank的横坐标
	int x = 0;
	// 表示tank的纵坐标
	int y = 0;

	public Tank(int x, int y) {
		this.x = x;
		this.y = y;
	}
}

class Hero extends Tank {
	public Hero(int x, int y) {
		super(x, y);
	}
}
